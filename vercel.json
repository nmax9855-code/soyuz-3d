{
  "name": "soyuz-3d",
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/pages/(.*)",
      "dest": "/pages/$1",
      "status": 200
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "status": 200
    }
  ]
}
